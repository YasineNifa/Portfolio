{
  "name": "YassineNifaPortfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "apollo-boost": "^0.4.4",
    "colorthief": "^2.3.0",
    "gh-pages": "^2.1.1",
    "graphql": "^14.5.8",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-dotenv": "^0.1.3",
    "react-easy-emoji": "^1.3.0",
    "react-headroom": "^3.0.0",
    "react-lottie": "^1.2.3",
    "react-reveal": "^1.2.2",
    "react-scripts": "^3.4.3",
    "react-twitter-embed": "3.0.3"
  },
  "react-dotenv": {
    "whitelist": [
      "REACT_APP_GITHUB_TOKEN",
      "REACT_APP_GITHUB_USERNAME"
    ]
  },
  "scripts": {
    "predeploy": "react-dotenv && npm run build",
    "start": "react-dotenv && react-scripts start",
    "build": "react-dotenv && react-scripts build",
    "deploy": "react-dotenv && gh-pages -b master -d build",
    "test": "react-dotenv && react-scripts test",
    "eject": "react-dotenv && react-scripts eject",
    "format": "prettier --write \"./**/*.{js,css,json}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
